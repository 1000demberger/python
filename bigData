      "source": [
        "# =====================================\n",
        "# 1) Instalar pacotes necessários\n",
        "# =====================================\n",
        "!pip install python-docx\n",
        "\n",
        "import pandas as pd\n",
        "import matplotlib.pyplot as plt\n",
        "from docx import Document\n",
        "from docx.shared import Inches\n",
        "from google.colab import files\n",
        "import csv\n",
        "\n",
        "plt.rcParams.update({'figure.max_open_warning': 0})\n",
        "\n",
        "# =====================================\n",
        "# 2) Upload do arquivo CSV\n",
        "# =====================================\n",
        "print(\"Selecione o arquivo CSV contendo apenas atendimentos improcedentes:\")\n",
        "uploaded = files.upload()\n",
        "\n",
        "filename = list(uploaded.keys())[0]\n",
        "print(\"Arquivo recebido:\", filename)\n",
        "\n",
        "# =====================================\n",
        "# 3) Detectar o separador automaticamente\n",
        "# =====================================\n",
        "with open(filename, \"r\", encoding=\"latin1\", errors=\"ignore\") as f:\n",
        "    sample = f.read(2048)\n",
        "    dialect = csv.Sniffer().sniff(sample)\n",
        "    sep = dialect.delimiter\n",
        "\n",
        "print(\"Separador detectado automaticamente:\", repr(sep))\n",
        "\n",
        "# =====================================\n",
        "# 4) Carregar o arquivo corretamente\n",
        "# =====================================\n",
        "df = pd.read_csv(filename, sep=sep, encoding=\"latin1\")\n",
        "print(\"\\nPrimeiras linhas da tabela carregada:\")\n",
        "display(df.head())\n",
        "\n",
        "print(\"Total de linhas:\", len(df))\n",
        "\n",
        "# =====================================\n",
        "# 5) Adicionar coluna improcedente = 1\n",
        "# =====================================\n",
        "df[\"improcedent_visit\"] = 1  # já que a base é só de improcedentes\n",
        "\n",
        "# =====================================\n",
        "# 6) Criar relatório Word\n",
        "# =====================================\n",
        "doc = Document()\n",
        "doc.add_heading(\"Relatório Comparativo de Atendimentos Improcedentes\", level=1)\n",
        "\n",
        "# Função para criar gráficos de contagem\n",
        "def create_plot(series, title, filename):\n",
        "    plt.figure(figsize=(10, 4))\n",
        "    series.plot(kind='bar')\n",
        "    plt.title(title)\n",
        "    plt.ylabel(\"Quantidade de improcedentes\")\n",
        "    plt.xticks(rotation=45, ha='right')\n",
        "    plt.tight_layout()\n",
        "    plt.savefig(filename)\n",
        "    plt.close()\n",
        "    return filename\n",
        "\n",
        "# =====================================\n",
        "# 7) Lista de análises (agora usando contagem!)\n",
        "# =====================================\n",
        "sections = {\n",
        "    \"Improcedentes por Localização (Urbano/Rural)\": (\"tipo_localizacao\", \"localizacao.png\"),\n",
        "    \"Improcedentes por Canal de Atendimento\": (\"canal_atendimento\", \"canal.png\"),\n",
        "    \"Improcedentes por Regional\": (\"regional\", \"regional.png\"),\n",
        "    \"Improcedentes por Distrital\": (\"distrital\", \"distrital.png\"),\n",
        "    \"Improcedentes por Seccional\": (\"seccional\", \"seccional.png\"),\n",
        "    \"Improcedentes por Causa\": (\"causa\", \"causa.png\"),\n",
        "    \"Improcedentes por Componente\": (\"componente\", \"componente.png\"),\n",
        "}\n",
        "\n",
        "# =====================================\n",
        "# 8) Criar gráficos e inserir no documento\n",
        "# =====================================\n",
        "for titulo, (coluna, img_name) in sections.items():\n",
        "    if coluna in df.columns:\n",
        "        series = df[coluna].value_counts()  # CORREÇÃO: agora é contagem, não média\n",
        "\n",
        "        doc.add_heading(titulo, level=2)\n",
        "        graph_file = create_plot(series, titulo, img_name)\n",
        "        doc.add_picture(graph_file, width=Inches(5))\n",
        "\n",
        "# =====================================\n",
        "# 9) Salvar relatório Word\n",
        "# =====================================\n",
        "output_filename = \"Relatorio_Improcedentes_Categoria.docx\"\n",
        "doc.save(output_filename)\n",
        "\n",
        "print(\"\\nRelatório gerado com sucesso:\", output_filename)\n",
        "\n",
        "# =====================================\n",
        "# 10) Download automático do arquivo\n",
        "# =====================================\n",
        "files.download(output_filename)\n"
      ],
